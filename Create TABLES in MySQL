CREATE TABLE memo(
    -> id INT(11) NOT NULL AUTO_INCREMENT,
    -> type VARCHAR(30) NOT NULL,
    -> tag VARCHAR(100) NULL,   
    -> title VARCHAR(100) NOT NULL,
    -> text TEXT NULL,
    -> date DATETIME NOT NULL,
    -> PRIMARY KEY(id));

CREATE TABLE tag(
    -> id INT(11) NOT NULL AUTO_INCREMENT,
    -> name VARCHAR(30
    -> ) NOT NULL,
    -> date DATETIME NOT NULL,
    -> PRIMARY KEY(id));
